import java.util.*;
public class sumofdivisor {
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int b=sc.nextInt();

        int sum=0;
        for(int i=1;i<=b;i++)
        {
            if(b%i==0)
            {
                sum+=i;
            }
        }
        System.out.print(sum);
    }

}
