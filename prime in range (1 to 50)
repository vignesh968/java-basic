import java.util.*;
public class prime {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int c;
		for(int i=2;i<n;i++)
		{
		    c=0;
		    for(int j=2;j<=i/2;j++)
			{
			 if(i%j==0)
			 {
			c++;
			break;
			 }
			 			}
			 			if(c==0){
			 			    System.out.println(i);
			 			}
			 			
		}

	}

}
