
import java.util.*;
public class Main
{
  public static void main(String[]args)
  {
    Scanner s=new Scanner(System.in);
    int a=s.nextInt();
    int arr[][]=new int[10][10];
    for(int i=0;i<a;i++)
    {
      for(int j=0;j<a;j++)
      {
        arr[i][j]=s.nextInt();
      }
    }
    for(int i=0;i<a;i++)
    {
      for(int j=0;j<a;j++)
      {
        System.out.print(arr[j][i]+" ");
      }
    
    System.out.println();
  }
  }
}
