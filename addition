package basic;
import java.util.*;
public class Addition {

	public static void main(String[] args) {
		Scanner s=new Scanner(System.in);
		System.out.print("enter a number 1");

		int num1=s.nextInt();
		System.out.print("enter a number 2");
		int num2=s.nextInt();
		System.out.print(num1+num2);

	}

}
/*
output
enter a number 1
23
enter a number 2234
2578
*/
